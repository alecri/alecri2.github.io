@article{filippiniAssociationOutdoorAir2019,
  title = {Association between Outdoor Air Pollution and Childhood Leukemia: A Systematic Review and Dose–Response Meta-Analysis},
  shorttitle = {Association between Outdoor Air Pollution and Childhood Leukemia},
  author = {Filippini, Tommaso and Hatch, Elizabeth E. and Rothman, Kenneth J. and Heck, Julia E. and Park, Andrew S. and Crippa, Alessio and Orsini, Nicola and Vinceti, Marco},
  date = {2019-04-24},
  journaltitle = {Environmental Health Perspectives},
  shortjournal = {Environ Health Perspect},
  volume = {127},
