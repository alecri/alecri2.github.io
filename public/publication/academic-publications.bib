@article{larssonMilkConsumptionMortality2015,
  title = {Milk Consumption and Mortality from All Causes, Cardiovascular Disease, and Cancer: A Systematic Review and Meta-Analysis},
  shorttitle = {Milk Consumption and Mortality from All Causes, Cardiovascular Disease, and Cancer},
  author = {Larsson, Susanna C. and Crippa, Alessio and Orsini, Nicola and Wolk, Alicja and Michaëlsson, Karl},
  date = {2015-09},
  journaltitle = {Nutrients},
  volume = {7},
